  	<display-name>purchaseinvoice</display-name>
  	<description></description>
  	<servlet-class>gstdemo.purchaseinvoice</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>salesinvoice</servlet-name>
  	<display-name>salesinvoice</display-name>
  	<description></description>
  	<servlet-class>gstdemo.salesinvoice</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>signin</servlet-name>
  	<url-pattern>/signin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>signup</servlet-name>
  	<url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>listvendormaster</servlet-name>
  	<url-pattern>/listvendormaster</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>listsalesinvoice</servlet-name>
  	<url-pattern>/listsalesinvoice</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>listpurchaseinvoice</servlet-name>
  	<url-pattern>/listpurchaseinvoice</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>listprofile</servlet-name>
  	<url-pattern>/listprofile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>listmaterialmaster</servlet-name>
  	<url-pattern>/listmaterialmaster</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>purchaseinvoice</servlet-name>
  	<url-pattern>/purchaseinvoice</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>salesinvoice</servlet-name>
  	<url-pattern>/salesinvoice</url-pattern>

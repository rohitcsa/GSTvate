<?xml version="1.0" encoding="UTF-8"?>
<web-app>
<session-config>
        <session-timeout>
            -1
        </session-timeout>
    </session-config>
    <context-param>
        <description>JDBC Host Name</description>
        <param-name>host</param-name>
        <param-value>jdbc:mysql://localhost:3306/gstdemo</param-value>
    </context-param>
    <context-param>
        <description>JDBC Driver</description>
        <param-name>driver</param-name>
        <param-value>com.mysql.jdbc.Driver</param-value>
    </context-param>
    <context-param>
        <description>JDBC Username</description>
        <param-name>userName</param-name>
        <param-value>root</param-value>
    </context-param>
    <context-param>
        <description>JDBC Password</description>
        <param-name>password</param-name>
        <param-value>2309</param-value>
    </context-param>
    <context-param>
        <description>OpenShift</description>
        <param-name>OSflag</param-name>
        <param-value>0</param-value>
    </context-param>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>signin</servlet-name>
  	<display-name>signin</display-name>
  	<description></description>
  	<servlet-class>gstdemo.signin</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>signup</servlet-name>
  	<display-name>signup</display-name>
  	<description></description>
  	<servlet-class>gstdemo.signup</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>listvendormaster</servlet-name>
  	<display-name>listvendormaster</display-name>
  	<description></description>
  	<servlet-class>gstdemo.listvendormaster</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>listsalesinvoice</servlet-name>
  	<display-name>listsalesinvoice</display-name>
  	<description></description>
  	<servlet-class>gstdemo.listsalesinvoice</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>listpurchaseinvoice</servlet-name>
  	<display-name>listpurchaseinvoice</display-name>
  	<description></description>
  	<servlet-class>gstdemo.listpurchaseinvoice</servlet-class>
  </servlet>
  <servlet>
